@{
	ViewBag.Title = "Two";
}

<h2>Two</h2>

<h1>Arrays</h1>

@section scripts {
	<script src="@Url.Content("/Scripts/lessons/two.js")" type="text/javascript"></script>
}

<!-- ko foreach: Employees -->

<div class="row">
	<div class="col-md-3">
		<label for="name">Name: </label>
		<input name="name" type="text" data-bind="value: name" />
	</div>
	<div class="col-md-3">
		<label for="title">Title: </label>
		<input name="title" type="text" data-bind="value: title" />
	</div>
	<div class="col-md-3">
		<label for="company">Company: </label>
		<input name="company" type="text" data-bind="value: company" />
	</div>
	<div class="col-md-3">
		<button  class="btn btn-primary" data-bind="click: Promote">Promote!</button>
	</div>
</div>
<div class="row">
	<h2 data-bind="text: signature"></h2>
</div>

<hr />

