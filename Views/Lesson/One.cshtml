@{
	ViewBag.Title = "One";
}

<h2>One</h2>

@section scripts {
	<script src="@Url.Content("/Scripts/lessons/one.js")" type="text/javascript"></script>
}
<div class="row">
	<div class="col-md-6">
		<label for="name">Name: </label>
		<input name="name" type="text" data-bind="value: name" />
	</div>
	<div class="col-md-6">
		<label for="title">Title: </label>
		<input name="title" type="text" data-bind="value: title" />
	</div>
</div>
<div class="row">
	<div class="col-md-6">
		<label>Company: </label>
		<input type="text" data-bind="value: company" />
	</div>
	<div class="col-md-6">
		<button  class="btn btn-primary" data-bind="click: Promote">Promote!</button>
	</div>
</div>
<div class="row">
	<h2 data-bind="text: signature"></h2>
</div>





